Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ",
   RowBox[{"Grid", " ", "Parameters"}], " ", "*)"}], "\n",
  RowBox[{
   RowBox[{
    RowBox[{"step", " ", "=", " ", "0.001"}], ";", "  ",
    RowBox[{"(*", " ",
     RowBox[{
     "Reduce", " ", "this", " ", "to", " ", "get", " ", "a", " ", "more", " ",
       "precise", " ", "estimate"}], " ", "*)"}], "\n",
    RowBox[{"xmin", "=",
     RowBox[{"-", "1.7"}]}], ";"}], "\n",
   RowBox[{
    RowBox[{"xmax", "=", "1.7"}], ";"}], "\n",
   RowBox[{
    RowBox[{"ymin", "=", "0"}], ";"}], "\n",
   RowBox[{
    RowBox[{"ymax", "=", "1"}], ";"}], "\[IndentingNewLine]",
   "\[IndentingNewLine]",
   RowBox[{"(*", " ",
    RowBox[{"Estimate", " ", "sofaHeight", " ", "for", " ", "all", " ", "x"}],
     " ", "*)"}], "\[IndentingNewLine]",
   RowBox[{
    RowBox[{"heightData", "=",
     RowBox[{"Table", "[",
      RowBox[{
       RowBox[{"{",
        RowBox[{"x", ",",
         RowBox[{"Sum", "[",
          RowBox[{
           RowBox[{
            RowBox[{"Boole", "[",
             RowBox[{"isInsideGerverSofa", "[",
              RowBox[{"{",
               RowBox[{"x", ",", "y"}], "}"}], "]"}], "]"}], "*", "step"}],
           ",",
           RowBox[{"{",
            RowBox[{"y", ",",
             RowBox[{"ymin", " ", "+", " ",
              RowBox[{"(",
               RowBox[{"step", "/", "2"}], ")"}]}], ",",
             RowBox[{"ymax", " ", "-", " ",
              RowBox[{"(",
               RowBox[{"step", "/", "2"}], ")"}]}], ",", "step"}], "}"}]}],
          "]"}]}], "}"}], ",",
       RowBox[{"{",
        RowBox[{"x", ",",
         RowBox[{"xmin", " ", "+", " ",
          RowBox[{"(",
           RowBox[{"step", "/", "2"}], ")"}]}], ",",
         RowBox[{"xmax", "-", " ",
          RowBox[{"(",
           RowBox[{"step", "/", "2"}], ")"}]}], ",", "step"}], "}"}]}],
      "]"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]",
   RowBox[{"(*", " ",
    RowBox[{"Calculate", " ", "and", " ", "print", " ", "result"}], " ",
    "*)"}], "\[IndentingNewLine]",
   RowBox[{
    RowBox[{"estimatedArea", " ", "=", " ",
     RowBox[{"Sum", "[",
      RowBox[{
       RowBox[{"step", "*",
        RowBox[{"(",
         RowBox[{"item", "[",
          RowBox[{"[", "2", "]"}], "]"}], ")"}]}], ",",
       RowBox[{"{",
        RowBox[{"item", ",", "heightData"}], "}"}]}], "]"}]}], ";", "  ",
    RowBox[{"(*", " ",
     RowBox[{
      RowBox[{
      "Estimated", " ", "area", " ", "of", " ", "the", " ", "2", "D", " ",
       "Gerver", " ", "sofa"}], ",", " ",
      RowBox[{"for", " ", "accuracy", " ", "comparison"}]}], " ", "*)"}],
    "\[IndentingNewLine]",
    RowBox[{"estimatedVolume", "=",
     RowBox[{"Sum", "[",
      RowBox[{
       RowBox[{"step", "*",
        RowBox[{
         RowBox[{"(",
          RowBox[{"item", "[",
           RowBox[{"[", "2", "]"}], "]"}], ")"}], "^", "2"}]}], ",",
       RowBox[{"{",
        RowBox[{"item", ",", "heightData"}], "}"}]}], "]"}]}], ";", " ",
    RowBox[{"(*", " ",
     RowBox[{
     "Estimated", " ", "volume", " ", "of", " ", "the", " ", "XY", " ",
      "Gerver", " ", "sofa", " ", "intersected", " ", "with", " ", "the", " ",
       "XZ", " ", "Gerver", " ", "sofa"}], " ", "*)"}], "\[IndentingNewLine]",
    RowBox[{"Print", "[",
     RowBox[{"\"\<Step size: \>\"", ",", " ", "step"}], "]"}], ";"}],
   "\[IndentingNewLine]",
   RowBox[{
    RowBox[{"Print", "[",
     RowBox[{
     "\"\<Estimated Area of 2D Gerver sofa for comparison: \>\"", ",", " ",
      "estimatedArea", ",", " ", "\"\< (Diff from Exact Area: \>\"", ",", " ",
      RowBox[{"Abs", "[",
       RowBox[{"estimatedArea", " ", "-", " ", "exactArea"}], "]"}], ",",
      " ", "\"\<)\>\""}], "]"}], ";"}], "\[IndentingNewLine]",
   RowBox[{
    RowBox[{"Print", "[",
     RowBox[{
     "\"\<Estimated Volume of extruded XY Gerver sofa intersected with \
extruded XZ Gerver sofa: \>\"", ",", "estimatedVolume"}], "]"}],
    ";"}]}]}]], "Input",
